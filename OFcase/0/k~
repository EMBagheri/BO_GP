FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "k_IC"

dimensions      [0 2 -2 0 0 0 0];

internalField $k_internalField;

boundaryField
{
    inlet_low
    {
        type            fixedValue;
        value           $k_inflow_low;
    }
    inlet_up
    {
        type            fixedValue;
        value           $k_inflow_up;
    }
    
    outlet_low
    {
        type            zeroGradient;
    }
    outlet_up
    {
        type            zeroGradient;
    }
    
    topWall
    {
      type            zeroGradient;//symmetryPlane;
        value           uniform 1.e-15;
    }
    
    bottomWall
    {
        type            fixedValue;
        value           uniform 1.e-15;
    }
    
    front_low
    {
        type            empty;
    }
    back_low
    {
        type            empty;
    }
    front_up
    {
        type            empty;
    }
    back_up
    {
        type            empty;
    }
}


