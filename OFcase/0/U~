FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "U_IC"

dimensions      [0 1 -1 0 0 0 0];

// internalField   nonuniform List<vector>
// (
// );

internalField $U_internalField;

boundaryField
{
    inlet_low
    {
        type            fixedValue;
        // value           nonuniform List<vector>
	//   (
	//    );
	value $U_inflow_low;
    }
    inlet_up
    {
        type            fixedValue;
        // value           nonuniform List<vector>
	  // (
	  //  );
		value $U_inflow_up;
    }

    outlet_low
    {
        type            zeroGradient;
    }
    outlet_up
    {
        type            zeroGradient;
    }

    topWall
    {
        type            zeroGradient;
    }

    bottomWall
    {
        type            noSlip;
    }

    front_low
    {
        type            empty;
    }
    back_low
    {
        type            empty;
    }
    front_up
    {
        type            empty;
    }
    back_up
    {
        type            empty;
    }
}

