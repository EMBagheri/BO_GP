FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet_low
    {
        type            zeroGradient;
    }
    inlet_up
    {
        type            zeroGradient;
    }

    outlet_low
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet_up
    {
        type            fixedValue;
        value           uniform 0;
    }

    topWall
    {
      type            fixedValue;//symmetryPlane;
      value           uniform 0;
    }

    bottomWall
    {
        type            zeroGradient;
    }

    front_low
    {
        type            empty;
    }
    back_low
    {
        type            empty;
    }
    front_up
    {
        type            empty;
    }
    back_up
    {
        type            empty;
    }
}

